<ul>
  <% @students.each do |student| %>
  <li>
    <p>Name: <%=student.first_name() %> <%=student.second_name()%></p>
    <p>House: <%= student.find_house.name %></p>
    <p>Age: <%= student.age() %></p>
  </li><hr>
  <form action="/hogwarts/<%=student.id %>/delete" method="post">
  <input type="submit" value="Delete"/>
  </form>
  <form action="/hogwarts/<%= student.id %>/edit" method="get"> <input type="submit" value="Edit"/>

  </form>

  <%end %>
</ul>
